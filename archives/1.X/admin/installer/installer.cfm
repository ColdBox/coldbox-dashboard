<!--- Determine type of installation ---><cfparam name="setupPacket" type="xml"><!--- Instantiate installer ---><cfset objInstaller = CreateObject("component","Installer").init(setupPacket)><html><head><title ><cfoutput>#objInstaller.getTitle()#</cfoutput></title><style type="text/css">body{	font-family: Arial, Helvetica, sans-serif;	font-size: 10px;	background-color:#E2E4D9}h2,h3{	font-family: Arial, Helvetica, sans-serif;	font-size:16px;	font-weight: bold;	color: #000000;	padding: 0px;}.errorMessage{	padding: 10px;	border: 1px solid #999999;	font-size: 12px;	font-weight: bold;	color: #A10B07;	width:  80%;	margin: auto;	margin-top: 20px;	background-color: #ffff99;}</style></head><body><div align="center" style="margin-top: 10%"><h2><cfoutput>#objInstaller.getMessage()#</cfoutput></h2><object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" 		codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab##version=7,0,19,0" width="158" height="22">        <param name="movie" value="load.swf" /><param name="quality" value="high" /><embed src="load.swf" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="158" height="22"></embed></object><br>Please Wait...</div><cfflush><!--- Start Installtion ---><cfset rtnResults = objInstaller.startInstall()><!--- Check For Errors ---><cfif rtnResults.error>	<cfoutput>#objInstaller.errorHandler()#</cfoutput><cfelse>	<cfoutput>#objInstaller.successHandler()#</cfoutput></cfif></body></html>