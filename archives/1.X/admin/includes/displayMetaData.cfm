<!--- display metadata for a component ----><cfparam name="attributes.metaData" type="any"><cfparam name="attributes.path" type="any"><cfset md = attributes.metaData><cfparam name="md.Hint" default=""><cfparam name="md.Extends" default="#StructNew()#"><cfset aMethods = md.Functions><!--- check if there are any TO DO comments ---><cffile action="read" file="#attributes.path#" variable="txtFile"><cfscript>	aTODO = ArrayNew(1);	iStartPos = FindNoCase("TO DO:",txtFile);	while(iStartPos gt 0) {		iEndPos = FindNoCase("*/", txtFile, iStartPos);		if(iEndPos gt 0)			aTODO[ArrayLen(aTODO)+1] = Mid(txtFile,iStartPos+6,iEndPos-iStartPos-6);		iStartPos = Find("TO DO:",txtFile, iStartPos+6);	}</cfscript><cfif false><link href="style.css" rel="stylesheet" type="text/css" /></cfif><cfoutput><hr /><br /><a name="#md.Name#"></a><cfif not caller.valueExists("cfdoctype") ><div align="right"><strong>Print:</strong> <a href="#cgi.script_name#?event=#caller.controller.getValue('event')#&cfdoctype=pdf&type=#caller.controller.getValue("type")#" target="_blank"><img src="images/i_pdf.gif" border="0" align="absbottom"/></a> <a href="#cgi.script_name#?event=#caller.controller.getValue('event')#&cfdoctype=flashpaper&type=#caller.controller.getValue("type")#" target="_blank"><img src="images/flashpaper.gif" border="0" align="absbottom"/></a><br /><br /></div></cfif>	<table width="100%" style="border:1px solid ##000000;">		<tr valign="top">			<td colspan="3" class="titlesDarkBg">			<div class="dashboardTitles">#md.Name#</div>	 		  </td>		</tr>		   <tr valign="top" >		  <td colspan="3" class="TablesCells">#md.Hint#</td>	   </tr>			<tr valign="top" >		  <td colspan="3" class="fw_debugTablesTitlesDocs">Method Summary:</td>	   </tr>	  <cfloop from="1" to="#ArrayLen(aMethods)#" index="i">	<cfset thisMethod = aMethods[i]>	<cfparam name="thisMethod.Name" default="">	<cfparam name="thisMethod.Hint" default="">	<cfparam name="thisMethod.Access" default="public">	<cfparam name="thisMethod.ReturnType" default="">	<cfparam name="thisMethod.Parameters" default="#ArrayNew(1)#">	<cfif thisMethod.Access eq "">		<cfset thisMethod.Access = "public">	</cfif>	<!--- Parameters --->	<cfset aParams = thisMethod.Parameters>	<cfset lstParams = "">	<cfloop from="1" to="#ArrayLen(aParams)#" index="j">		<cfset thisParam = aParams[j]>		<cfparam name="thisParam.Name" 		default="">		<cfparam name="thisParam.Required" 	default="true" type="boolean">		<cfparam name="thisParam.Type" 		default="">		<cfparam name="thisParam.Default"	default="">				<cfset tmpParam = "#thisParam.Type# <b>#thisParam.Name#</b>">		<cfif Not thisParam.Required>			<cfset tmpParam = "&nbsp;[#tmpParam# = '#thisParam.Default#' ]">		</cfif>				<cfset lstParams = ListAppend(lstParams, tmpParam)>							</cfloop>	<tr valign="top">		<td width="105" align="right" class="TablesTitles">#thisMethod.Access# <cfif thisMethod.ReturnType neq "">#thisMethod.ReturnType#</cfif></td>		<td  class="TablesCells">			<b>#thisMethod.Name#</b>			<cfif lstParams neq "">				( #lstParams# )			<cfelse>				()			</cfif>			<br />			<p>#thisMethod.Hint#</p>			<cfif arrayLen(aParams) gt 0>			<p><strong>Parameters:</strong><br />			<cfloop from="1" to="#ArrayLen(aParams)#" index="j">				<cfset thisParam = aParams[j]>				<cfparam name="thisParam.Name" 		default="">				<cfparam name="thisParam.Required" 	default="true" type="boolean">				<cfparam name="thisParam.Type" 		default="">				<cfparam name="thisParam.Default"	default="">						<cfset tmpParam = "#thisParam.Type# <b>#thisParam.Name#</b>">				<cfif Not thisParam.Required>					<cfset tmpParam = "[#tmpParam# = '#thisParam.Default#' ]">				</cfif>						#j#. #tmpParam# <cfif structKeyExists(thisParam, "hint")>- <em>#thisParam.hint#</em></cfif>				<br />							</cfloop>			</p>			</cfif>		</td>	</tr></cfloop><tr valign="top" >  <td colspan="3" class="fw_debugTablesTitlesDocs">Extends:</td></tr><tr valign="top" >	<td colspan="3" class="TablesCells">		<cfif IsSimpleValue(md.Extends)>			<ul><li>#md.Extends#</li></ul>		<cfelseif IsDefined("md.Extends.Name") and md.Extends.Name neq "WEB-INF.cftags.component">			<ul><li>#md.Extends.Name#</li></ul>		<cfelse>			<ul><li><em>Nothing</em></li></ul>		</cfif>	</td></tr>  </table>  <div align="center">  <bR />  <a href="##thetop">^ Top</a>  </div>  <br /></cfoutput>